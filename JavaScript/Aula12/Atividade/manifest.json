{
    "name": "ImagensBR",
    "version": "1.0",
    "description": "Extensao para contar e alterar as imagens",
    "content_scripts": [
        {
            "matches": ["*://*.br/*", "*://*.com.br/*"],
            "js": ["pages.js"]
        }
